
@model TechnoKala.Areas.Panel.Model.Team.EditViewModel


<div class="content-wrapper" style="min-height: 916.281px;">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            صفحه خالی
            <small>آماده برای پروژه شما</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> خانه</a></li>
            <li><a href="#">مثال ها</a></li>
            <li class="active">صفحه خالی</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">

        <!-- Default box -->
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">مثال ساده</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form asp-area="Panel" asp-controller="Team" asp-action="Edit" method="post">
                <div class="box-body">
                    <div class="form-group">
                        <label for="exampleInputEmail1">  نام و نام خانوادگی</label>
                        <input value="@Model.fullname" asp-for="fullname" type="text" class="form-control" id="exampleInputEmail1" placeholder=" نام و نام خانوادگی ">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1"> مسئولیت  </label>
                        <input value="@Model.title" asp-for="title" type="text" class="form-control" id="exampleInputEmail1" placeholder="مسئولیت ">
                    </div>
               
               

                    <div class="checkbox">
                        <label>
                            <input type="checkbox"> مرا به خاطر بسپار
                        </label>
                    </div>
                </div>
                <!-- /.box-body -->

                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">ارسال</button>
                </div>
            </form>
        </div>
        <!-- /.box -->

    </section>
    <!-- /.content -->
</div>